.venue__venue
  position absolute
  bottom 140px
  left calc(50vw + 250px)
  z-index 2
  line-height 1
  @media only screen and (max-width: md)
    left calc(50vw + 160px)
    bottom 90px
  @media only screen and (max-width: sm)
    z-index 4
    transform translateX(-50%)
    left 50%
    width 200px
    bottom 75px
    