.partnership__img
  position absolute
  top -300px
  right 0
  width 500px
  z-index 2
  @media only screen and (max-width: 1380px)
    width 400px
    top -180px
  @media only screen and (max-width: lg)
    width 300px
    top -180px
    margin-right -50px
  @media only screen and (max-width: sm)
    width 200px
    top 0
  @media only screen and (max-width: xs)
    display none